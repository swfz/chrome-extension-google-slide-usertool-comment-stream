{
  "manifest_version": 3,
  "name": "Rpce React Vite Example",
  "version": "1.0.0",
  "permissions": ["storage", "activeTab", "scripting", "downloads"],
  "background": {
    "service_worker": "background/background.ts"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/presentation/d/*/edit"],
      "js": ["content_script/stream.ts"]
    },
    {
      "matches": ["https://docs.google.com/presentation/d/e/*"],
      "js": ["content_script/subscribe.ts"]
    }
  ],
  "action": { "default_popup": "index.html" }
}
